set (seed:22)

--LOAD DATA
read ("3.5_t10_len100_preprocessed.fasta")

--SEARCH SETTINGS
set (partitioncharacter:#)
set (criterion:parsimony)
set (graphtype:tree)
build()
swap()

--REPORT RESULTS
report ("3.5_t10_len100_searchstats.csv", search) 
report ("3.5_t10_len100_searchTrees.nwk", graphs, newick, nohtulabels)
select (unique)
select (best)
report ("3.5_t10_len100_bestUnique.nwk", graphs, newick, nohtulabels)
report ("3.5_t10_len100_ia.tnt", tnt, concatenate, overwrite)
report ("3.5_t10_len100_bestConsensus.nwk", newick, reconcile, method:strict, nohtulabels)
