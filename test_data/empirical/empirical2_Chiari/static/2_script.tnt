log 2_concatenated.log
mxram 2048;
taxname +90;
nstates gaps;
proc 2_concatenated_noPreprocessing.tnt;
outgroup protopterus;
hold 10000;
xmult= level 7 chklevel 3 consense 3;
best;
tsave *2_concatenated_results.out;
