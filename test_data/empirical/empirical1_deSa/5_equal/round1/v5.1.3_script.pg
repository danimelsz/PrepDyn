set (seed:73412305)

--LOAD DATA
read ("output_H1_aligned_GB2MSA_preprocessed.fasta")
read ("output_COI_aligned_GB2MSA_preprocessed.fasta")
read ("output_RAG1_A_aligned_GB2MSA_preprocessed.fasta")
read ("output_RAG1_B_aligned_GB2MSA_preprocessed.fasta")

-- REPORT LOADED DATA
report ("v5.1.3_PHYG_Sa_crossref.csv", crossrefs, overwrite)
report ("v5.1.3_PHYG_Sa.data", data, overwrite)

--LOAD INITIAL TREES
read ("v2.1_POY_Sa.nwk")

--SEARCH SETTINGS
set (partitioncharacter:#)
set (criterion:parsimony)
set (graphtype:tree)
set (outgroup:Cycloramphus_boraceiensis)
search (hours:12, thompson:2, instances:5, exponential)

--REPORT RESULTS
report ("v5.1.3_PHYG_Sa_stats.txt", search)
report ("v5.1.3_PHYG_sa_searchTrees.txt", graphs, newick, nohtulabels)

select (unique)
select (best)
report ("v5.1.3_PHYG_Sa_bestUnique.nwk", graphs, newick, nohtulabels)
report ("v5.1.3_PHYG_Sa_ia.tnt", tnt, concatenate, overwrite)
report ("v5.1.3_PHYG_Sa_bestConsensus.nwk", newick, reconcile, method:strict)
