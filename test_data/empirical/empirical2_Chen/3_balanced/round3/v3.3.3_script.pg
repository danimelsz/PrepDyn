set (seed:73412305)

--LOAD DATA
read ("*.fasta")

-- REPORT LOADED DATA
report ("v3.3.3_PHYG_crossref.csv", crossrefs, overwrite)
report ("v3.3.3_PHYG.data", data, overwrite)

--LOAD INITIAL TREES
read ("v3.3.1_POY.nwk")

--SEARCH SETTINGS
set (partitioncharacter:#)
set (criterion:parsimony)
set (graphtype:tree)
set (outgroup:Cycloramphus_boraceiensis)
search (hours:12, thompson:2, instances:5, exponential)

--REPORT RESULTS
report ("v3.3.3_PHYG_stats.txt", search)
report ("v3.3.3_PHYG_searchTrees.txt", graphs, newick, nohtulabels)

select (unique)
select (best)
report ("v3.3.3_PHYG_bestUnique.nwk", graphs, newick, nohtulabels)
report ("v3.3.3_PHYG_ia.tnt", tnt, concatenate, overwrite)
report ("v3.3.3_PHYG_bestConsensus.nwk", newick, reconcile, method:strict)
